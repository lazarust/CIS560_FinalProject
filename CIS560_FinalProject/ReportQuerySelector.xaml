<UserControl x:Class="CIS560_FinalProject.ReportQuerySelector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CIS560_FinalProject"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button Name="Query1" Click="Query1_Click">Report Query  1</Button>
        <Button Name="Query2" Grid.Column="1" Click="Query2_Click">Report Query 2</Button>
        <Button Name="Query3" Grid.Column="2" Click="Query3_Click">Report Query 3</Button>
        <Button Name="Query4" Grid.Column="3" Click="Query4_Click">Report Query 4</Button>
    </Grid>
</UserControl>
